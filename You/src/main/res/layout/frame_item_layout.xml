<co.geeksempire.frames.you.Utils.Views.ViewGroups.NextedConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:apps="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rootViewItem"
    android:layout_width="match_parent"
    android:layout_height="283dp"
    android:layout_marginTop="37dp"
    android:layout_marginHorizontal="11dp"

    apps:layout_constraintDimensionRatio="1:1">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/frameBackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/primaryColorRed"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/frameForeground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="@color/primaryColorPurple" />

</co.geeksempire.frames.you.Utils.Views.ViewGroups.NextedConstraintLayout>